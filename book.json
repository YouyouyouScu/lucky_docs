{
    "title": "高先生的博客",
    "author": "Blankj",
    "description": "Mrgao blogs",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "高先生": "https://github.com/mrgaogang"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "highlight",
        "back-to-top-button",
        "expandable-chapters-small",
        "github",
        "github-buttons",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon",
        "-lunr",
        "-search",
        "search-pro",
        "tbfed-pagefooter",
        "page-toc-button",
        "klipse",
        "donate",
        "theme-api",
        "pageview-count"
    ],
    "pluginsConfig": {
    
        "github": {
            "url": "https://github.com/mrgaogang"
        },
        "github-buttons": {
            "buttons": [{
                "user": "mrgaogang",
                "repo": "lucky_docs",
                "type": "star",
                "size": "small",
                "count": true
            }]
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },

        "sharing": {

            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true


        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon": {
            "shortcut": "./source/images/favicon.jpg",
            "bookmark": "./source/images/favicon.jpg",
            "appleTouch": "./source/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./source/images/apple-touch-icon.jpg",
                "180x180": "./source/images/apple-touch-icon.jpg"
            }
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy gaogang 2019",
            "modify_label": "文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "donate": {
            "wechat": "/images/wechat.jpeg",
            "alipay": "/images/alipay.jpeg",
            "button": "赏赐",
            "alipayText": "支付宝捐赠",
            "wechatText": "微信捐赠"
          }
    }
}